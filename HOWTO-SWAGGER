mkdir swagger-gen

curl -X POST https://generator3.swagger.io/api/generate -H 'content-type: application/json' -d '{"specURL" : "http://goelzer.org/kalshi-swagger3.json", "lang" : "go", "type" : "CLIENT", "codegenVersion" : "V3"}' --output swagger-gen/outfile.zip

cd swagger-gen

unzip outfile.zip

cd ..

mv swagger-gen pkg/kalshi/swagger
